{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "outDir": "static/js",
    "target": "es2016",
    "declaration": false,
    "module": "es6",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "sourceMap": true,
  },
  "include": [
    "src/**/*"
  ]
}
